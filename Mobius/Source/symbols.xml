    -->
    <Parameter name='defaultSyncSource' values='default,none,track,out,host,midi,transport'
               options='noBinding'
               coreName='syncSource'
               codeName='SyncSource'
               enumName='SyncSource'/>
    <Parameter name='defaultTrackSyncUnit' values='default,subcycle,cycle,loop'
               options='noBinding'
               codeName='SyncTrackUnit'
               enumName='SyncTrackUnit'/>
    <Parameter name='slaveSyncUnit' values='beat,bar'
               options='noBinding'
               codeName='SyncUnit'
               enumName='SyncUnit'/>

    <!-- Don't need to clutter the symbol table with these, they have
